    "# Capacidad: maximo creditos del alumno;\n",
    "# Cantidad de cursos habilitados para matricula;\n",
    "# Valor: {20, 100, 50, 5, 30};\n",
    "# Peso: {5, 19, 11, 1, 20};"
   ]
